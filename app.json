{
  "expo": {
    "name": "googleauth",
    "slug": "googleauth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "REVERSED_CLIENT_ID": "com.googleusercontent.apps.636500952504-vf4rja6uh1sfj3p137554c2rsg5qi6ha",
        "GIDServerClientID": "636500952504-vf4rja6uh1sfj3p137554c2rsg5qi6ha.apps.googleusercontent.com"
      }
    },
    "plugins": ["@react-native-google-signin/google-signin"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
